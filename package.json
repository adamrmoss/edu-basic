{
    "name": "edu-basic",
    "version": "0.0.1",
    "description": "Education-focused BASIC dialect that runs in web browser",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "npm run build:worklet && ng serve",
        "build": "npm run build:worklet && ng build",
        "build:prod": "npm run build:worklet:prod && ng build --configuration production",
        "build:worklet": "node build-worklet.mjs --sourcemap",
        "build:worklet:prod": "node build-worklet.mjs --minify",
        "build:worklet:watch": "node build-worklet.mjs --watch --sourcemap",
        "watch": "npm run build:worklet && ng build --watch --configuration development",
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:watch": "jest --watch"
    },
    "private": true,
    "dependencies": {
        "@angular/cdk": "19.x.x",
        "@angular/common": "19.x.x",
        "@angular/compiler": "19.x.x",
        "@angular/core": "19.x.x",
        "@angular/forms": "19.x.x",
        "@angular/platform-browser": "19.x.x",
        "@angular/platform-browser-dynamic": "19.x.x",
        "jszip": "3.x.x",
        "ng-luna": "0.x.x",
        "rxjs": "7.x.x",
        "tslib": "2.x.x",
        "zone.js": "0.x.x"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "19.x.x",
        "@angular/cli": "19.x.x",
        "@angular/compiler-cli": "19.x.x",
        "@types/jest": "29.x.x",
        "@types/jszip": "3.x.x",
        "esbuild": "0.x.x",
        "jest": "29.x.x",
        "jest-preset-angular": "14.x.x",
        "typescript": "5.x.x"
    }
}
